<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="shop" class="com.gopi.mapsetter.Shop">
	
		<property name="id" value="1"></property>
		<property name="name" value="krishna"></property>
		<property name="loc" value="KSR Bengalore"></property>
		<property name="items" >
			<map>
				<entry key="1" value="shoes"/>
				<entry key="2"  value="Shirt"/>
				<entry key="3"  value="Pant"/>
				<entry key="4"  value="Watch"/>
			</map>
		
		</property>
	</bean>
    

</beans>